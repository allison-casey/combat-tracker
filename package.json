{
  "name": "combat-tracker",
  "version": "1.0.0",
  "description": "DnD Combat Tracker for AP based game systems",
  "main": "resources/main.js",
  "scripts": {
    "dev": "./node_modules/.bin/shadow-cljs watch main renderer",
    "build": "./node_modules/.bin/shadow-cljs compile main renderer",
    "clean": "rm -rf resources/public/js/* && rm -rf target",
    "release": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "combat.tracker",
    "mac": {
      "category": "combat.tracker.category"
    }
  },
  "keywords": [
    "shadow-cljs",
    "reagent",
    "electron"
  ],
  "author": "Ahonn Jiang <ahonn95@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.2.5",
    "electron-builder": "^22.8.0",
    "shadow-cljs": "^2.10.22",
    "source-map-support": "^0.5.19"
  },
  "dependencies": {
    "bootstrap-icons": "^1.0.0-alpha5",
    "create-react-class": "^15.6.3",
    "electron-store": "^6.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "scheduler": "^0.19.1",
    "ws": "^7.3.1",
    "xregexp": "^4.3.0",
    "yaml": "^1.10.0"
  }
}
